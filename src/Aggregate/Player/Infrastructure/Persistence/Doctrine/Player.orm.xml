<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                        https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Aggregate\Player\Domain\Player" table="player">
        <embedded name="id" class="App\Aggregate\Player\Domain\ValueObject\PlayerId" use-column-prefix="false" />
        <embedded name="clubId" class="App\Aggregate\Club\Domain\ValueObject\ClubId" use-column-prefix="true" />
        <embedded name="federatedCode" class="App\Aggregate\Player\Domain\ValueObject\PlayerFederatedCode" use-column-prefix="false" />
        <embedded name="name" class="App\Aggregate\Player\Domain\ValueObject\PlayerFirstName" use-column-prefix="false" />
        <embedded name="surname" class="App\Aggregate\Player\Domain\ValueObject\PlayerSurname" use-column-prefix="false" />
        <embedded name="birthdate" class="App\Aggregate\Player\Domain\ValueObject\PlayerBirthdate" use-column-prefix="false" />
        <embedded name="active" class="App\Aggregate\Player\Domain\ValueObject\PlayerActive" use-column-prefix="false" />
        <embedded name="synced" class="App\Aggregate\Player\Domain\ValueObject\PlayerSynced" use-column-prefix="false" />
    </entity>
</doctrine-mapping>